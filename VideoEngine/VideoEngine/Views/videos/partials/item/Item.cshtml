@model Jugnoon.Videos.Models.VideoItemViewModel
@*
    Core Item Design
*@

@{
    var PartialPath = "~/Views/videos/partials";
}

@if (Model.ListObject.ListType == Jugnoon.Scripts.ListType.Grid)
{
    @await Html.PartialAsync(PartialPath + "/item/gridItem.cshtml", Model);
}
else
{
    @await Html.PartialAsync(PartialPath + "/item/listItem.cshtml", Model);
}

